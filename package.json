{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "sanitize.css": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "standard": {
    "ignore": [
      "build/*"
    ],
    "parser": "babel-eslint"
  }
}
